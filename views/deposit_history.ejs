<h3>Deposit History</h3>
    
    <div class="row mb-30">
        <div class="col-xl-3 text-center">
            Number of deposits            <div class="white-box mt-2">
                <%= depositHistory ? depositHistory.length : 0 %>
            </div>
        </div>
        <div class="col-xl-3 text-center">
            Total deposited amount            <div class="white-box mt-2">
                $<%= depositHistory && depositHistory.length > 0 ? depositHistory.reduce((total, d) => total + d.amount, 0).toFixed(2) : '0.00' %>
            </div>
        </div>
        <div class="col-xl-3 text-center">
            Pending deposits            <div class="white-box mt-2">
                <%= depositHistory ? depositHistory.filter(d => d.status === 'pending').length : 0 %>
            </div>
        </div>
        <div class="col-xl-3 text-center">
            Completed deposits            <div class="white-box mt-2">
                <%= depositHistory ? depositHistory.filter(d => d.status === 'completed').length : 0 %>
            </div>
        </div>
    </div>

    <div class="content-box table-responsive">
	
	<table class="tab">
					<thead>
						<tr>
							<th>Type</th>
							<th>Amount</th>
							<th>Currency</th>
							<th>Status</th>
							<th>Date</th>
							<th>Transaction ID</th>
							<th>Payment Method</th>
						</tr>
					</thead>
					<tbody>
					
					<% if (depositHistory && depositHistory.length > 0) { %>
						<% depositHistory.forEach(function(deposit) { %>
							<tr>
								<td>Deposit</td>
								<td><%= deposit.amount %></td>
								<td><%= deposit.currency.toUpperCase() %></td>
								<td>
									<span class="badge badge-<%= deposit.status === 'completed' ? 'success' : deposit.status === 'pending' ? 'warning' : deposit.status === 'failed' ? 'danger' : 'secondary' %>">
										<%= deposit.status.charAt(0).toUpperCase() + deposit.status.slice(1) %>
									</span>
								</td>
								<td><%= new Date(deposit.date).toLocaleDateString() %></td>
								<td><%= deposit.transactionId || 'N/A' %></td>
								<td><%= deposit.paymentMethod || 'N/A' %></td>
							</tr>
						<% }); %>
					<% } else { %>
						<tr>
							<td colspan="7" align="center"><b>No deposit transactions found</b></td>
						</tr>
					<% } %>
																			 
						
						
						
						
											</tbody>
				</table>
 

